{
	"name": "@ctx-core/dev-tools",
	"version": "8.2.157",
	"description": "ctx-core dev-tools",
	"keywords": [
		"ctx-core",
		"dev-tools"
	],
	"homepage": "https://github.com/ctx-core/dev-tools#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/dev-tools/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/dev-tools.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./bin"
	],
	"types": "./index.d.ts",
	"bin": {
		"generate-index-ts": "./bin/generate-index-ts/index.sh"
	},
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test:unit && pnpm test:types",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"@ctx-core/dir": "^8.2.121",
		"ctx-core": "^5.34.0",
		"readline": "^1.3.0",
		"tsx": "^4.7.0"
	},
	"devDependencies": {
		"@types/glob-stream": "^8.0.2",
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tslib": "^2.6.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}